<Window x:Class="Tema_4_Ejercicio_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema_4_Ejercicio_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel HorizontalAlignment="Center" Height="414" VerticalAlignment="Center" Width="780" LastChildFill="False">
            <Label Content="Label" Width="0"/>
            <Label x:Name="Label1" Content="Margen Superior" DockPanel.Dock="Top" Height="100" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
            <Label Content="Label" Width="0"/>
            <Label x:Name="Label2" Content="Margen Inferior" DockPanel.Dock="Bottom" Height="100" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
            <Button x:Name="Boton1" Content="Botón Izquierdo" Width="100"/>
            <Button x:Name="Boton2" Content="Botón Derecho" DockPanel.Dock="Right" Width="100"/>
        </DockPanel>

    </Grid>
</Window>
